{{#each items as |item|}}
  <div class="menu-item" data-id="{{item.id}}" data-price="{{item.cost}}">
    <button type="button" class="btn btn-success menu-button-add"  data-id="{{item.id}}"><i class="fa fa-plus-square" data-id="{{item.id}}" aria-hidden="true"></i></button>
    <span class="menu-item-span" data-id="{{item.id}}">{{item.name}} - ${{item.cost}}</span>
</div>
{{/each}}
